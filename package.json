  },
  "scripts": {
    "start": "react-scripts start",
    "start:server": "node src/server/server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:node": "mocha -w './src/server/**/*.test.js'",
    "eject": "react-scripts eject",
    "prettier": "prettier --jsx-bracket-same-line --tab-width 4 --write 'src/**/**/*.js'",
    "dev": "npm run dev:client & npm run dev:server",
    "dev:client": "npm run start",
    "dev:server": "nodemon src/server/server.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
