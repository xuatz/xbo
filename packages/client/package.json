{
  "name": "client",
  "version": "3.0.0-alpha.1",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--no-experimental-fetch react-scripts --openssl-legacy-provider start",
    "dev": "npm run start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/**",
    "prettier": "prettier --jsx-bracket-same-line --tab-width 4 --write 'src/**/**/*.js'"
  },
  "eslintConfig": {
    "plugins": [
      "react-hooks"
    ],
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "axios": "0.23.0",
    "date-fns": "2.25.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "react-scripts": "4.0.3",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0",
    "styled-components": "2.1.2"
  },
  "devDependencies": {
    "@xuatz/eslint-config": "workspace:~",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "should": "13.2.3",
    "typescript": "^4.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
